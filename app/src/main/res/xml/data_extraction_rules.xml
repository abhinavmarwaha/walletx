<?xml version="1.0" encoding="utf-8"?>

<!-- https://developer.android.com/about/versions/12/backup-restore#xml-changes -->

<data-extraction-rules>
    <cloud-backup>
        <include domain="root" />
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />
        <include domain="external" />
    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <include domain="file" />
        <include domain="database" />
        <include domain="sharedpref" />
        <include domain="external" />
    </device-transfer>
</data-extraction-rules>